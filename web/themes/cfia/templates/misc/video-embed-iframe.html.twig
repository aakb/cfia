/**
 * @file
 * Display an iframe with alterable components.
 */
#}

{% if url is not empty %}
  <iframe{{ attributes.setAttribute('data-category-consent', 'cookie_cat_statistic') }} src="" data-consent-src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}" frameborder="0" allowfullscreen=""></iframe>
{% endif %}
